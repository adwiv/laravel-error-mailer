{
    "name": "adwiv/laravel-error-mailer",
    "description": "Report errors by mail while ignoring repeating ones.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": ">=10.0",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Adwiv\\Laravel\\ErrorMailer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adwiv\\Laravel\\ErrorMailer\\ErrorMailerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
